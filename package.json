    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-markdown": "6.0.2",
    "react-syntax-highlighter": "15.4.3",
    "rehype-raw": "5.1.0",
    "remark-gfm": "1.0.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "11.2.0",
